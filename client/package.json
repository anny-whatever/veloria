{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "node server.js",
    "build": "npm run generate-sitemap && vite build",
    "build:ssr": "npm run generate-sitemap && node scripts/build-ssr.js",
    "build:analyze": "vite build --mode analyze",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "optimize:js": "node scripts/optimize-js.js",
    "optimize:images": "node scripts/optimize-images.js",
    "build:production": "npm run generate-sitemap && npm run optimize:js && vite build && npm run optimize:images",
    "build:ssr:production": "npm run generate-sitemap && npm run optimize:js && node scripts/build-ssr.js && npm run optimize:images",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve:ssr": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "axios": "^1.8.3",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.487.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.18.0",
