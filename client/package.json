    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.29",
    "rollup-plugin-visualizer": "^5.9.2",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.3",
    "terser": "^5.19.4",
    "vite": "^4.4.5"
